#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules/vite/bin/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules/vite/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules/vite/bin/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules/vite/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/vite@6.1.6_@types+node@24.2_14a972d9d4965f8014ef713b25dc9889/node_modules:/mnt/c/Sheencity/Workspace/cmand-doc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
